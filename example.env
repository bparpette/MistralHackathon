# Configuration optimisée pour AWS Lambda
# Timeout de 30s maximum

# Qdrant - Configuration minimale
QDRANT_URL=<VOTRE_QDRANT_URL>
QDRANT_API_KEY=<VOTRE_QDRANT_API_KEY>
QDRANT_ENABLED=true

# Supabase - Configuration obligatoire
SUPABASE_URL=<VOTRE_SUPABASE_URL>
SUPABASE_SERVICE_ROLE_KEY=<VOTRE_SUPABASE_SERVICE_ROLE_KEY>

# MCP Server - Configuration optimisée
MCP_SERVER_PORT=3000
MCP_SERVER_DEBUG=false

# Optimisations Lambda
AWS_LAMBDA_FUNCTION_NAME=<VOTRE_LAMBDA_FUNCTION_NAME>
AWS_EXECUTION_ENV=<VOTRE_EXECUTION_ENV>
LAMBDA_TASK_ROOT=<VOTRE_LAMBDA_TASK_ROOT>